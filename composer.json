{
    "name": "delphi/parser-scoperesolver",
    "description": "PHPParser Visitor to resolve and set scope",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Sartain",
            "email": "tomsartain@gmail.com"
        }
    ],
    "require": {
        "delphi/parser-fileresolver": "dev-main",
        "delphi/parserutils": "dev-main",
        "nikic/php-parser": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "delphi\\Parser\\Scope\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "delphi\\Tests\\Parser\\Scope\\": "tests/"
        }
    }
}
